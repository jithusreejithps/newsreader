</body>
<script>
    $('#b1').click(function () {

       debugger;
       var entity = {};
       entity.language = $('#lang').val();
        entity.category = $('#category').val();
        entity.news_provider = $('#provider').val();
        entity.rss_feed = $('#t3').val();

        var dt = JSON.stringify(entity);
        
        $.ajax({
            type: "post",
            url: "/admin/add_cat",
            data: dt,
            dataType: "json",
            contentType: "Application/json",
            success: function (a) {
                debugger;
                if (a == "exist") {
                    sweetAlert("Oops...", "this Already Exists...", "error");
                }
                else {
                    swal("Nice!", "You Have Added A New prrovider ", "success");
                }
            }
        });
    });

    $.ajax({
        type: "post",
        url: "/common/list_category",
        data: {},
        dataType: "json",
        contentType: "Application/json",
        success: function (a) {
            
            var dt = JSON.parse(a);
            ko.applyBindings(dt, document.getElementById('category'));
        }
    });
    $.ajax({
        type: "post",
        url: "/common/list_language",
        data: {},
        datatype: "json",
        contentType: "application/json",
        success: function (result) {
            debugger;
            var dt = JSON.parse(result);
            ko.applyBindings(dt, document.getElementById('lang'));
        }
    });
    $('#lang').change(function () {
        debugger;
        var entity = {};
        entity.lang = $('#lang').val();


        var dt = JSON.stringify(entity);
        debugger;
        $.ajax({
            type: "post",
            url: "/common/selectprovider",
            data: dt,
            dataType: "json",
            contentType: "Application/json",
            success: function (a) {
                var dt = JSON.parse(a);
                ko.applyBindings(dt, document.getElementById('provider'));
            }
        });
    });


    $('#logout').click(function () {

        swal({
            title: "Are you sure?",
            text: "You Really Want To Logout ?",
            type: "warning",
            showCancelButton: true,
            confirmButtonColor: "#DD6B55",
            confirmButtonText: "Yes, Logout!",
            cancelButtonText: "No, cancel plx!",
            closeOnConfirm: false,
            closeOnCancel: false
        }, function (isConfirm) {
            if (isConfirm) {
                swal("LoggedOut!", "See You Soon", "success");
                window.location.href = "/guest/index";
            }
            else {
                swal("Cancelled", "Your Really Brilliant :)", "error");

            }
        });
    });
   
</script>
</html>